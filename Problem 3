//The prime factors of 13195 are 5, 7, 13 and 29.
//
//What is the largest prime factor of the number 600851475143 ?
//
//Found on https://projecteuler.net/problem=3
//
// SOLUTION

#include <iostream>
using namespace std;

int main()
{
int max = 1;
int temp = 1;
  for (int n = 1; n < 600851475143; n++)
  {
    if (600851475143%n==0)
    {
      temp = n;
      for (int g = 1; g < n; g++)
      {
        if (max%n == 0) // this shyt does not work. Why the fuck did I think so would work? Fuck I look like
        {
          max = temp;
        }
      }
    }
  }
cout << max;
return 0;
}
